/* pages/index/index.wxss */
page{
  background: #F7F7F6
}

.selectIcon{
  height:54rpx;
  background: #ffffff;
  border-top-left-radius: 40rpx;
  border-bottom-left-radius: 40rpx;
  width: 8%;
  display: flex;
  /* justify-content: center; */
  align-items: center;
}

.inputPlaceholder{
  border-top-right-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
  font-size:14px;
  background:#ffffff;
  color:#bfbfbf;
}

.selectView{
  width: 100%;
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
}

.selIconImg{
  width: 30rpx;
  height: 30rpx;
  margin-left: 10px;
}

.bottomFont{
  display: flex;
  justify-content: center;
  font-size: 20rpx;
  color: #919191;
}