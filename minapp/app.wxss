/**tab切换样式**/
.active{
    color:#F65959;
    border-bottom: 4rpx solid #F65959;
}
.table {
  display:table;
  width:100%;
}
.menu_tab {
  background:#fff;
  z-index:999;
  top:0;
  font-size:28rpx;
  color:#666;
  height:90rpx;
  border-bottom:2rpx solid #eee;
}
.cell {
  display:table-cell;
  vertical-align:middle;
}
.menu_tab .cell {
  color:#666;
  text-align:center;
}
.menu_tab .active {
  color: red
}

.blank{
  height: 10rpx;
  background-color: #F7F7F6;
}
/**攻略列表通用样式  **/
.content_list{
  margin: 0 20rpx 80rpx 20rpx;
  overflow-y: auto
}
.content_list .content_list_item{
  background-color: white;
  border-top-left-radius:20rpx;
  border-top-right-radius:20rpx;
  margin:20rpx 0;
}
.content_list .content_list_item .item_img{
  border-top-left-radius:20rpx;
  border-top-right-radius:20rpx;
}
.content_list .content_list_item .rank_name{
  display:table;
  margin:0.2rem 0 0 0;
  padding:0 20rpx;
  width:100%;
  border-bottom: 1px solid #efefef 
}
.content_list .content_list_item .rank_name .cell_ico{
  display:table-cell;
  vertical-align:middle;
  padding:0;
  margin:0;
  width:12vw;
}
.content_list .content_list_item .rank_name .cell_nickName{
  display:table-cell;
  vertical-align:middle;
  padding:0;
  margin:0;
  font-size:3.5vw;
  color:#641045;
}
.content_list .content_list_item .rank_name .cell_right{
  display:table-cell;
  vertical-align:bottom;
  text-align:right;
  padding-right:20rpx;
  margin:40rpx 20rpx 0 0;
  font-size:3.5vw;
  color:#c4c4c4;
  float: right
}
.content_list .content_list_item .rank_name .cell_right .btn_a{
  color:#999;
  font-size:3vw;
  margin:0 0 0 20rpx;
  display:inline-table;
}
.content_list .content_list_item .rank_name .cell_right .btn_a i{
  color:#999;
  font-size:3vw;
  margin:0 0.05rem 0 0;
}
.content_list .content_list_item .cont_text{
  display: table;
  padding: 20rpx
}
.content_list .content_list_item .cont_text .cont_text_title{
  display: table;
  width: 100%
}
.content_list .content_list_item .cont_text .cont_text_title .cell_title{
  display: table;
  font-size:4vw;
}
.content_list .content_list_item .cont_text .cont_text_title .cell_tags{
  display: table-cell;
  font-size:18rpx;
  color:white;
}
.content_list .content_list_item .cont_text .cont_text_title .cell_tags .item_tag{
  display: inline-block;
  margin: 0 4rpx;
  background-color:#3695E6;
  border-radius:20rpx;
  width:70rpx;
  height:28rpx;
  text-align:center;
}
.content_list .content_list_item .cont_text .cont_text_title .cell_time{
  display: table-cell;
  vertical-align:middle;
  text-align:right;
  padding:0;
  margin:0;
  font-size:3.5vw;
  color:#c4c4c4;
}
.content_list .content_list_item .cont_text .cont_text_desc{
  color:#333;
  font-size:28rpx;
  line-height:1.75;
  max-height:140rpx;
  overflow:hidden;
  word-wrap:break-word;
  word-break:break-all;
  margin:20rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
}
.ico_img{
  width: 24rpx;
  height: 20rpx;
  margin-right: 10rpx
}
/**攻略列表样式结束**/

